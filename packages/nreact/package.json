{
  "name": "@texonom/nreact",
  "version": "1.3.0-rc.3",
  "type": "module",
  "description": "Fast and accurate React renderer for Notion.",
  "repository": "texonom/notion-node",
  "author": "Alan Jo <sungle3737@gmail.com>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
